                                                @Html.TextBoxFor(model => model.Taxcode, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Payment Term:</label>
                                                @Html.TextBoxFor(model => model.Terms, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Supplier Status:</label>
                                                @Html.TextBoxFor(model => model.Status, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Created By:</label>
                                                @Html.TextBoxFor(model => model.Recuser, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Created Date:</label>
                                                @Html.TextBoxFor(model => model.Recdate, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Modified By:</label>
                                                @Html.TextBoxFor(model => model.Moduser, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group">
                                                <label class="col-lg-4" style="text-align:right; font-weight:normal;">Date Modified:</label>
                                                @Html.TextBoxFor(model => model.ModDate, new { @class = "form-control", @style = "width:350px", @readonly = "readonly" })
                                            </div>

                                        </div>
                                        <!-- /.col-lg-12 (nested) -->
                                    </div>
                                    <!-- /.row (nested) -->

        </div>

            <div class="form-group">
                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
            </div>
    </div>
@Html.Action("SupplierScripts", "Suppliers")
 
